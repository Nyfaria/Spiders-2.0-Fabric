{
  "schemaVersion": 1,
  "id": "spiderstpo",
  "version": "${version}",

  "name": "Spiders 2.0",
  "description": "Fabric port of the Mod Nyf's Spiders which is a Fork and Update of Spiders 2.0",
  "authors": [
    "Nyfaria"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.nyfaria.spiderstpo.SpidersTPO"
    ]
  },
  "mixins": [
    "spiderstpo.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener" : "spiderstpo.accesswidener"
}
